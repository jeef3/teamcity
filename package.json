{
  "name": "teamcity",
  "version": "1.0.0-preview.2",
  "description": "TeamCity API for NodeJS",
  "license": "MIT",
  "repository": "jeef3/teamcity",
  "maintainers": [
    "Jeff Knaggs <mail@jeef3.com>"
  ],
  "scripts": {
    "build": "tsc",
    "build:w": "tsc --watch --pretty",
